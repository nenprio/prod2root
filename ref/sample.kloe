              CCle_Cry(iCCLE) = 0.
              CCle_Det(iCCLE) = 0.
              CCle_Col(iCCLE) = 0.
              CCle_Pla(iCCLE) = 0.
              CCle_Time(iCCLE) = 0.
            END DO
   
            istat = GETCCALTSTRU(Evt%CCLE)
            write(*,*) "ISTAT => ", istat
   
            nCCle = Evt%CCLE%NEle
            IF ( nCCle < 0 .OR. nCCle > 96 ) THEN
               WRITE(*,*) ERROR_BLOCK//'CCLE - nCCle '//ERROR_DOMAIN, nCCle
            END IF
   
            IF (nCCle > 0 .AND. nCCle <= 96) THEN
               DO iCCLE=1, nCCle
                  CCle_Cry(iCCLE) = Evt%CCLE%Cry(iCCLE)
                  IF ( CCle_Cry(iCCLE) < 0 .OR. CCle_Cry(iCCLE) > 96 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'CCLE - CCle_Cry '//ERROR_DOMAIN, CCle_Cry(iCCLE)
                  END IF
                    CCle_Det(iCCLE) = Evt%CCLE%Det(iCCLE)
                  CCle_Col(iCCLE) = Evt%CCLE%Col(iCCLE)
                  IF ( CCle_Col(iCCLE) < 0 .OR. CCle_Col(iCCLE) > 24 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'CCLE - CCle_Col '//ERROR_DOMAIN, CCle_Col(iCCLE)
                  END IF
                  CCle_Pla(iCCLE) = Evt%CCLE%Pla(iCCLE)
                  IF ( CCle_Pla(iCCLE) < 0 .OR. CCle_Pla(iCCLE) > 2 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'CCLE - CCle_Pla '//ERROR_DOMAIN, CCle_Pla(iCCLE)
                  END IF
                    CCle_Time(iCCLE) = Evt%CCLE%T(iCCLE)
              END DO
            ELSE
               WRITE(*,*) ERROR_BLOCK//'CCLE - nCCle '//ERROR_INDEX, nCCle
               nCCle = 0
            END IF
         ELSE
            WRITE(*,*) ERROR_MISS_DATA//'CCLE'
         END IF
      END IF
C-----------------------------------------------------------------------
C Fill Block lete
C-----------------------------------------------------------------------
      IF (LETEFLAG) THEN
         IF (.NOT.MONTECARLOFLAG) THEN
            LeteCalib = 0.
            nLete = 0.
            DO iLETE=1, 40
              Lete_Cry(iLETE) = 0.
              Lete_Det(iLETE) = 0.
              Lete_Col(iLETE) = 0.
              Lete_Pla(iLETE) = 0.
              Lete_E(iLETE) = 0.
              Lete_Time(iLETE) = 0.
            END DO
   
            istat = getletestru(evt%lete)
            write(*,*) "ISTAT => ", istat
   
            LeteCalib = Evt%LETE%Calib
            nLete = Evt%LETE%NEle
            IF ( nLete < 0 .OR. nLete > 40 ) THEN
               WRITE(*,*) ERROR_BLOCK//'LETE - nLete '//ERROR_DOMAIN, nLete
            END IF
   
            IF (nLete > 0 .AND. nLete <= 40) THEN
               DO iLETE=1, nLete
                  Lete_Cry(iLETE) = Evt%LETE%Cry(iLETE)
                  IF ( Lete_Cry(iLETE) < 0 .OR. Lete_Cry(iLETE) > 40 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'LETE - Lete_Cry '//ERROR_DOMAIN, Lete_Cry(iLETE)
                  END IF
                    Lete_Det(iLETE) = Evt%LETE%Det(iLETE)
                  Lete_Col(iLETE) = Evt%LETE%Col(iLETE)
                  IF ( Lete_Col(iLETE) < 0 .OR. Lete_Col(iLETE) > 4 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'LETE - Lete_Col '//ERROR_DOMAIN, Lete_Col(iLETE)
                  END IF
                  Lete_Pla(iLETE) = Evt%LETE%Pla(iLETE)
                  IF ( Lete_Pla(iLETE) < 0 .OR. Lete_Pla(iLETE) > 5 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'LETE - Lete_Pla '//ERROR_DOMAIN, Lete_Pla(iLETE)
                  END IF
                    Lete_E(iLETE) = Evt%LETE%E(iLETE)
                    Lete_Time(iLETE) = Evt%LETE%T(iLETE)
              END DO
            ELSE
               WRITE(*,*) ERROR_BLOCK//'LETE - nLete '//ERROR_INDEX, nLete
               nLete = 0
            END IF
         ELSE
            WRITE(*,*) ERROR_MISS_DATA//'LETE'
         END IF
      END IF
C-----------------------------------------------------------------------
C Fill Block itce
C-----------------------------------------------------------------------
      IF (ITCEFLAG) THEN
         IF (.NOT.MONTECARLOFLAG) THEN
            nITCE = 0.
            DO iITCE=1, 4032
              Foil(iITCE) = 0.
              ITLayer(iITCE) = 0.
              Strip(iITCE) = 0.
              View(iITCE) = 0.
              IndItKine(iITCE) = 0.
            END DO
   
            istat = ITCE_UPK(evt%itce)
            write(*,*) "ISTAT => ", istat
   
            nITCE = Evt%itce%nitce
            IF ( nITCE < 0 .OR. nITCE > 4032 ) THEN
               WRITE(*,*) ERROR_BLOCK//'ITCE - nITCE '//ERROR_DOMAIN, nITCE
            END IF
   
            IF (nITCE > 0 .AND. nITCE <= 4032) THEN
               DO iITCE=1, nITCE
                    Foil(iITCE) = Evt%itce%foil(iITCE)
                    ITLayer(iITCE) = Evt%itce%layer(iITCE)
                    Strip(iITCE) = Evt%itce%strip(iITCE)
                    View(iITCE) = Evt%itce%view(iITCE)
                    IndItKine(iITCE) = Evt%itce%inditkine(iITCE)
              END DO
            ELSE
               WRITE(*,*) ERROR_BLOCK//'ITCE - nITCE '//ERROR_INDEX, nITCE
               nITCE = 0
            END IF
         ELSE
            WRITE(*,*) ERROR_MISS_DATA//'ITCE'
         END IF
      END IF
C-----------------------------------------------------------------------
C Fill Block hete
C-----------------------------------------------------------------------
      IF (HETEFLAG) THEN
         IF (.NOT.MONTECARLOFLAG) THEN
            nHetDcs = 0.
            DO iHETE=1, 1920
              HDet(iHETE) = 0.
              HCol(iHETE) = 0.
              nTurnHet(iHETE) = 0.
              TimeHet(iHETE) = 0.
            END DO
   
            istat = gethetestru(evt%hete)
            write(*,*) "HETE ISTAT => ", istat

            ISTAT = BLOCAT(IW,'HETE', 1,IND1,INDDAT )
            write(*,*) "HETE BLOCAT => ", istat
   
            nHetDcs = Evt%hete%nhetdcs
            IF ( nHetDcs < 0 .OR. nHetDcs > 1920 ) THEN
               WRITE(*,*) ERROR_BLOCK//'HETE - nHetDcs '//ERROR_DOMAIN, nHetDcs
            END IF
   
            IF (nHetDcs > 0 .AND. nHetDcs <= 1920) THEN
               DO iHETE=1, nHetDcs
                  HDet(iHETE) = Evt%hete%hdet(iHETE)
                  IF ( HDet(iHETE) < 1 .OR. HDet(iHETE) > 2 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'HETE - HDet '//ERROR_DOMAIN, HDet(iHETE)
                  END IF
                  HCol(iHETE) = Evt%hete%hcol(iHETE)
                  IF ( HCol(iHETE) < 1 .OR. HCol(iHETE) > 32 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'HETE - HCol '//ERROR_DOMAIN, HCol(iHETE)
                  END IF
                  nTurnHet(iHETE) = Evt%hete%nturnhet(iHETE)
                  IF ( nTurnHet(iHETE) < 1 .OR. nTurnHet(iHETE) > 4 ) THEN
                     WRITE(*,*) ERROR_BLOCK//'HETE - nTurnHet '//ERROR_DOMAIN, nTurnHet(iHETE)
                  END IF
                    TimeHet(iHETE) = Evt%hete%timehet(iHETE)
              END DO
            ELSE
               WRITE(*,*) ERROR_BLOCK//'HETE - nHetDcs '//ERROR_INDEX, nHetDcs
               nHetDcs = 0
            END IF
         ELSE
            WRITE(*,*) ERROR_MISS_DATA//'ITCE'
         END IF
      END IF
C-----------------------------------------------------------------------
C Fill Root
C-----------------------------------------------------------------------
      call fillntu() 	
C==============================================================================
CC End of code
C==============================================================================
      RETURN
      END
